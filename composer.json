{
  "name": "atk4/chart",
  "type": "library",
  "description": "ChartJS for Agile UI",
  "keywords": [
    "agile",
    "data",
    "framework",
    "chart",
    "chartjs"
  ],
  "homepage": "https://github.com/atk4/chart",
  "license": "MIT",
  "authors": [
    {
      "name": "Romans Malinovskis",
      "email": "romans@agiletoolkit.org",
      "homepage": "https://nearly.guru/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "require": {
    "atk4/ui": "~2.3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpcov": "*",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "atk4\\chart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atk4\\chart\\tests\\": "tests/"
    }
  }
}
